local oldRead = file.Read
function file.Read( fn, path ) 
if string.find(fn, "javelin*") then
	idiot.surface.PlaySound("buttons/lightswitch2.wav")
    chat.AddText(Color( 255, 0, 0 ), "[", "IdiotBox", "] ", Color( 255, 255, 255 ), "Someone tried to steal a LUA file from you! Sucessfully blocked!")
else
    return oldRead( fn, path )
end
end -- Blocks LUA file stealing

local type = type

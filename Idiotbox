	surface.DrawRect(pos.x - w / 2 - 7, pos.y - h - 1, 5, h + 2);
	surface.SetDrawColor(col2);
	surface.DrawRect(pos.x - w / 2 - 6, pos.y - h + diff, 3, hp);
	end
